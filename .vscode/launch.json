	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": true
		},
		{
			"name": "auto_graph RELEASE",
			"type": "python",
			"request": "launch",
			"python": "${workspaceFolder}/external/cpython/PCBuild/amd64/python.exe",
			"program": "${workspaceFolder}/auto_graph/auto_graph.py",
			"console": "integratedTerminal",
			"justMyCode": true,
			"preLaunchTask": "build extension RELEASE"
		},
		{
			"name": "auto_graph DEBUG",
			"type": "python",
			"request": "launch",
			"python": "${workspaceFolder}/external/cpython/PCBuild/amd64/python_d.exe",
			"program": "${workspaceFolder}/auto_graph/auto_graph.py",
			"console": "integratedTerminal",
			"justMyCode": true,
			"preLaunchTask": "build extension DEBUG"
		}
	]
}